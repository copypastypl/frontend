{
    "name": "copypasty",
    "description": "",
    "version": "0.0.1",
    "scripts": {
        "dev": "run-p dev:*",
        "dev:css": "postcss static/tailwind.pcss -o static/tailwind.css -w & postcss static/global.pcss -o static/global.css -w",
        "dev:routify": "routify",
        "dev:nollup": "nollup -c --verbose",
        "build": "run-s build:*",
        "build:css": "postcss static/global.pcss -o static/global.css",
        "build:routify": "routify -b",
        "build:rollup": "rollup -c"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@roxi/routify": "^2.18.0",
        "cssnano": "^5.0.5",
        "nollup": "^0.16.5",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.3.0",
        "postcss-cli": "^8.3.1",
        "postcss-import": "^14.0.2",
        "postcss-preset-env": "^6.7.0",
        "rollup": "^2.50.6",
        "rollup-plugin-hot": "^0.1.1",
        "rollup-plugin-svelte-hot": "^1.0.0-8",
        "rollup-plugin-terser": "^7.0.2",
        "stylelint": "^13.13.1",
        "stylelint-config-standard": "^22.0.0",
        "svelte": "^3.38.2",
        "svelte-preprocess": "^4.7.3",
        "tailwindcss": "^2.1.4"
    },
    "dependencies": {
        "@sveltejs/svelte-virtual-list": "^3.0.1",
        "@tailwindcss/typography": "^0.4.1",
        "svelte-awesome": "^2.3.2",
        "svelte-simple-modal": "^0.10.2"
    }
}
